/***/ "WOxb":
/*!*************************************************!*\
  !*** ./src/app/_components/_templates/utils.ts ***!
  \*************************************************/
/*! exports provided: getAllFields, filterForFieldValueList */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "getAllFields", function() { return getAllFields; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "filterForFieldValueList", function() { return filterForFieldValueList; });
//  This file is adapted from Nebula/Constellation components/Templates/utils.js
function getAllFields(pConnect) {
    const metadata = pConnect.getRawMetadata();
    let allFields = [];
    if (metadata.children && metadata.children.map) {
        allFields = metadata.children.map((fields) => {
            const children = fields.children instanceof Array ? fields.children : [];
            return children.map((field) => field.config);
        });
    }
    return allFields;
}
function filterForFieldValueList(fields) {
    return fields
        .filter(({ visibility }) => visibility !== false)
        .map(({ value, label }) => ({
        id: label.toLowerCase(),
        name: label,
        value
    }));
}


/***/ }),


/*! exports provided: TextComponent */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "TextComponent", function() { return TextComponent; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "mrSG");
/* harmony import */ var _raw_loader_text_component_html__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! raw-loader!./text.component.html */ "wK9l");
/* harmony import */ var _text_component_scss__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ./text.component.scss */ "qd7O");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! @angular/core */ "fXoL");
/* harmony import */ var _helpers_utils__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ../../../_helpers/utils */ "WQxO");
/* harmony import */ var _bridge_angular_pconnect__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! ../../../_bridge/angular-pconnect */ "40g3");






// import * as moment from "moment";
let TextComponent = class TextComponent {
    constructor(angularPConnect, utils) {
        this.angularPConnect = angularPConnect;
        this.utils = utils;
        this.label$ = "";
        this.value$ = "";
        this.bRequired$ = false;
        this.bReadonly$ = false;
        this.bDisabled$ = false;
        this.bVisible$ = true;
        this.componentReference = "";
        this.format$ = "text";
        this.formattedUrl$ = "";
        // Used with AngularPConnect
        this.angularPConnectData = {};
    }
    ngOnInit() {
        // First thing in initialization is registering and subscribing to the AngularPConnect service
        this.angularPConnectData = this.angularPConnect.registerAndSubscribeComponent(this, this.onStateChange);
        // Then, continue on with other initialization
        // call updateSelf when initializing
        this.checkAndUpdate();
    }
    ngOnDestroy() {
        if (this.angularPConnectData.unsubscribeFn) {
            this.angularPConnectData.unsubscribeFn();
        }
    }
    checkAndUpdate() {
        // Should always check the bridge to see if the component should
        // update itself (re-render)
        const bUpdateSelf = this.angularPConnect.shouldComponentUpdate(this);
        // ONLY call updateSelf when the component should update
        if (bUpdateSelf) {
            this.updateSelf();
        }
    }
    // updateSelf
    updateSelf() {
        // moved this from ngOnInit() and call this from there instead...
        this.configProps$ = this.pConn$.resolveConfigProps(this.pConn$.getConfigProps());
        if (this.configProps$["value"] != undefined) {
            this.value$ = this.configProps$["value"];
        }
        this.label$ = this.configProps$["label"];
        // TDB - get formats
        switch (this.formatAs$) {
            case "text":
                this.formattedValue$ = this.value$;
                break;
            case "date":
                this.formattedValue$ = this.generateDate(this.value$);
                break;
            case "date-time":
                this.formattedValue$ = this.generateDateTime(this.value$);
                break;
            case "time":
                if (this.value$) {
                    const timeParts = this.value$.split(":");
                    this.formattedValue$ = `${timeParts[0]}:${timeParts[1]}`;
                }
                else {
                    this.formattedValue$ = "";
                }
                break;
            case "url":
                this.formattedUrl$ = this.generateUrl(this.value$);
                this.formattedValue$ = this.value$;
                break;
        }
    }
    // Callback passed when subscribing to store change
    onStateChange() {
        this.checkAndUpdate();
    }
    generateUrl(sVal) {
        if (sVal.indexOf("https://") == 0 || sVal.indexOf("http://") == 0) {
        }
        else {
            // assume no http
            sVal = "http://" + sVal;
        }
        return sVal;
    }
    generateDate(sVal) {
        if (!sVal)
            return "";
        // const value = new Intl.DateTimeFormat('default', {
        //   year: 'numeric',
        //   month: 'numeric',
        //   day: 'numeric'
        // }).format(new Date(sVal + "T00:00"));
        // sVal = moment(sVal, "YYYYMMDD").format("MM/DD/YYYY");
        return this.utils.generateDate(sVal, "Date-Long-Custom-YYYY");
    }
    generateDateTime(sVal) {
        if (!sVal)
            return "";
        if (sVal.length === 10)
            return this.generateDate(sVal);
        let value = sVal.substring(0, sVal.length - 1);
        // value = new Intl.DateTimeFormat('default', {
        //   year: 'numeric',
        //   month: 'numeric',
        //   day: 'numeric',
        //   hour: 'numeric',
        //   minute: 'numeric',
        //   second: 'numeric',
        //   hour12: true,
        // }).format(new Date(value))
        // sVal = moment(sVal, "YYYYMMDD").format("MM/DD/YYYY, hh:mm a");
        return this.utils.generateDateTime(value, "DateTime-Long-YYYY-Custom");
    }
};

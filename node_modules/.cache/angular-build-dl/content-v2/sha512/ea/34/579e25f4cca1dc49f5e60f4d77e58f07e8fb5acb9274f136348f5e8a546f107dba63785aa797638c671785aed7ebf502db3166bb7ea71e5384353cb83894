        this.bShowLinkModal$ = false;
        this.bShowViewAllModal$ = false;
        this.arFileMainButtons$ = [];
        this.arFileSecondaryButtons$ = [];
        this.arLinkMainButtons$ = [];
        this.arLinkSecondaryButtons$ = [];
        this.arFiles$ = [];
        this.arFileList$ = [];
        this.arLinks$ = [];

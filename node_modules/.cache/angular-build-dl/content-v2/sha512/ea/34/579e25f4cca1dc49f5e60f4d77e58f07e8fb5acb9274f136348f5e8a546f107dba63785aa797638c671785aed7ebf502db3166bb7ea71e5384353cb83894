    datasource$: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_3__["Input"] }],
    headerText$: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_3__["Input"] }],
    itemKey$: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_3__["Input"] }],
    showTodoList$: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_3__["Input"] }],
    target$: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_3__["Input"] }],

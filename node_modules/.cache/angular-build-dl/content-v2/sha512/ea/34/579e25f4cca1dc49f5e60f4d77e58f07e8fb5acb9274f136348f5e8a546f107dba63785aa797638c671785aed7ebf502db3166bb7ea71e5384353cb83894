/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony default export */ __webpack_exports__["default"] = ("<app-material-details [arFields$]=\"arFields$\" [arFields2$]=\"arFields2$\" [arFields3$]=\"arFields3$\"></app-material-details> \n");


/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "CancelAlertComponent", function() { return CancelAlertComponent; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "mrSG");
/* harmony import */ var _raw_loader_cancel_alert_component_html__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! raw-loader!./cancel-alert.component.html */ "hTHf");

let DetailsTwoColumnComponent = class DetailsTwoColumnComponent {
    constructor(angularPConnect) {
        this.angularPConnect = angularPConnect;
        this.arFields$ = [];
        this.arFields2$ = [];
        this.arFields3$ = [];
        // Used with AngularPConnect
        this.angularPConnectData = {};
